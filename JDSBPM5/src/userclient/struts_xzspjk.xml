<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="xzspjk" extends="struts-default">
		<action name="XZSPAction_*" class="com.kzxd.xzsp.action.XZSPAction" method="{1}">
			<result name="findAllXZSP" type="redirect">XZSPAction_findAllXZSP.action</result>
		</action>
		
		<action name="LctzAction_*" class="com.kzxd.xzsp.action.LctzAction" method="{1}">
			<result name="success">/xzspjk/jsp/tzlc.jsp</result>
		</action>
		
	</package>
</struts>    